[include]
    path = ~/.gitconfig.secret

[alias]
    co = checkout
    cob = checkout -b
    undo = reset --soft HEAD^
    cm = commit -m

[pull]
    rebase = false

[push]
    default = simple

[status]
    showUntrackedFiles = all

[user]
    name = seongwoong 
    email = seongwoong@mobilint.com

[url "git@github.com:"]
    insteadOf = https://github.com/

[url "git@git.mobilint.com:"]
    insteadOf = https://git.mobilint.com/

[credential]
	helper = cache
